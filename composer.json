{
  "name": "anton-am/json-ld",
  "description": "Extremely simple JSON-LD markup generator.",
  "keywords": [
    "json-ld",
    "generator",
    "schema",
    "structured-data"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    },
    {
      "name": "Anton Am",
      "email": "antonamru@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0|^7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.3",
    "phpstan/phpstan": "^0.12.14",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "AntonAm\\JsonLD\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AntonAm\\JsonLD\\Test\\": "tests"
    }
  },
  "archive": {
    "exclude": [
      "/docs",
      "/tests"
    ]
  }
}
